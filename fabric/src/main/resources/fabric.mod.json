{
  "schemaVersion": 1,
  "id": "enderitemod",
  "version": "${version}",
  "name": "Enderite Mod",
  "description": "Adds Enderite tools and armor with void float ability. Translation: ES:CamoloQlo",
  "authors": [
    "PITTI11",
    "Nic4las"
  ],
  "contact": {
    "homepage": "",
    "sources": ""
  },
  "license": "CC0-1.0",
  "icon": "assets/enderitemod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.enderitemc.enderitemod.fabric.EnderiteModFabric"
    ],
    "client": [
      {
        "value": "net.enderitemc.enderitemod.fabriclike.EnderiteModClient"
      }
    ],
    "fabric-gametest": [
      "net.enderitemc.enderitemod.tests.EnderiteTests"
    ],
    "modmenu": [
      "net.enderitemc.enderitemod.fabriclike.config.EnderiteModModMenuIntegration"
    ],
    "shulkerboxtooltip": [
      "net.enderitemc.enderitemod.modIntegrations.ShulkerBoxTooltipApiImplementation"
    ]
  },
  "mixins": [
    "enderitemod.mixins.json",
    "enderitemod-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${architectury_version}",
    "fabric-api": ">=${fabric_api_version}",
    "cloth-config": ">=${cloth_config_version}"
  },
  "suggests": {
    "modmenu": ">=${mod_menu_version}",
    "shulkerboxtooltip": ">=${shulker_box_tooltip_version}"
  }
}